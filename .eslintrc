{
    "parser": "babel-eslint",
    "extends": [
        "google"
    ],
    "globals": {
        "__DEV__": true
    },
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "js": true
        }
    },
    "plugins": [
        "react",
        "jest",
        "styled-components",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error"
    },
    "javascript.validate.enable": false,
    "typescript.validate.enable": false
}